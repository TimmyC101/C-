@using MVCExercise.ViewModels
@model List<PersonVm>

<div class="container">
    <table style="width:75%" class="mx-auto">
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Quote</th>
        </tr>
        @foreach (var person in Model)
        {
        <tr>
            <td>@person.FirstName</td>
            <td>@person.LastName</td>
            <td>@person.EmailAddress</td>
            <td>$@person.Quote</td>
        </tr>
        }
    </table>
</div>

